<div class="@message.CSS">
    <div class="user">
        @message.Username
        <span title="@message.QueryInfoPlusTime"><MudIcon Icon="@Icons.Material.Filled.Info" Class="WhiteIcon" /></span>
    </div>
    <div class="msg">@message.Body</div>
</div>

@code {
    [Parameter]
    public MessageBubble message { get; set; }
}
